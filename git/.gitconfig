[user]
	name = Guilherme Pereira
	email = guilospanck@protonmail.com
	signingKey = <>
[url "https://Guilospanck:<>@github.com"] 
        insteadOf = https://github.com
[color]
	ui = auto
[core]
	editor = code --wait
	excludesFile = ~/.gitignore
[commit]
	gpgsign = true

[alias]
	tree = log --oneline --decorate --all --graph

[push]
	autoSetupRemote = true
	followTags = true
[merge]
	conflictStyle = zdiff3
[init]
	defaultBranch = main
[diff]
	algorithm = histogram
[branch]
	sort = -committerdate
